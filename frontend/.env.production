# 生产环境配置
# API配置
VITE_API_BASE_URL=https://api.ai-notebook.com
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# 应用配置
VITE_APP_NAME=AI智能记事本
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=AI驱动的智能记事本应用
VITE_APP_KEYWORDS=AI,记事本,笔记,项目管理,待办事项

# 功能开关
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_OFFLINE_MODE=true

# 性能配置
VITE_CACHE_DURATION=300000
VITE_MAX_CACHE_SIZE=50
VITE_LAZY_LOAD_THRESHOLD=0.1

# 安全配置
VITE_CSRF_PROTECTION=true
VITE_RATE_LIMIT=60
VITE_SESSION_TIMEOUT=3600000

# 第三方服务
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_HOTJAR_ID=HOTJAR_SITE_ID

# CDN配置
VITE_CDN_URL=https://cdn.ai-notebook.com
VITE_STATIC_URL=https://static.ai-notebook.com

# SEO配置
VITE_SITE_URL=https://ai-notebook.com
VITE_SITE_LOGO=https://static.ai-notebook.com/logo.png
VITE_SITE_FAVICON=https://static.ai-notebook.com/favicon.ico