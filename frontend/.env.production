# 生产环境配置
# 此文件用于Vercel部署

# =========================================
# API配置 - 连接Railway生产环境后端
# =========================================
VITE_API_BASE_URL=https://ai-notebook-backend-production.up.railway.app
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# =========================================
# 应用配置
# =========================================
VITE_APP_NAME=AI智能记事本
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=AI驱动的智能记事本应用
VITE_APP_ENV=production

# =========================================
# 功能开关 (生产环境)
# =========================================
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_OFFLINE_MODE=true

# =========================================
# 开发工具 (生产环境禁用)
# =========================================
VITE_ENABLE_REACT_DEVTOOLS=false
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_LOG_LEVEL=error

# =========================================
# 性能配置
# =========================================
VITE_CACHE_DURATION=300000
VITE_MAX_CACHE_SIZE=50
VITE_LAZY_LOAD_THRESHOLD=0.1

# =========================================
# 后端数据库信息 (仅用于显示)
# =========================================
VITE_DB_TYPE=supabase
VITE_DB_URL=https://lvwjycoderrjetuzqrdy.supabase.co
